<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE queryservice PUBLIC "-//ANYFRAME//DTD QUERYSERVICE//EN"
"http://www.anyframejava.org/dtd/anyframe-core-query-mapping-3.2.dtd">
<queryservice>
	<queries>	
		<query id="findGenericGenreList">
			<statement>
				SELECT genre_id, name
				FROM GENRE
				ORDER BY name
			</statement>
			<result class="org.anyframe.plugin.generic.domain.GenericGenre" />
		</query>
	</queries>
</queryservice>

